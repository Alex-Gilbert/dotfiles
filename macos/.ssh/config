Include /Users/alex/.colima/ssh_config

# Work SSH
Host w.github.com
  HostName github.com
  AddKeysToAgent yes
  User git
  IdentityFile ~/.ssh/id_rsa_work
  IdentitiesOnly yes

# Personal SSH
Host p.github.com
  HostName github.com
  AddKeysToAgent yes
  User git
  IdentityFile ~/.ssh/id_rsa_personal
  IdentitiesOnly yes

# Softserve
Host softadmin
    HostName nas
    Port 23231
    IdentityFile ~/.ssh/id_rsa_personal
    IdentitiesOnly yes

# Softserve
Host soft
    HostName gilberthouse
    Port 23231
    IdentityFile ~/.ssh/id_ed25519
    IdentitiesOnly yes

# Private Git Direct
Host nas
    HostName gilberthouse
    User adminadmin
    Port 22
