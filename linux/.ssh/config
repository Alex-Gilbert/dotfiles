# Work SSH
Host w.github.com
  HostName github.com
  AddKeysToAgent yes
  User git
  IdentityFile ~/.ssh/id_rsa_work
  IdentitiesOnly yes

# Personal SSH
Host p.github.com
  HostName github.com
  AddKeysToAgent yes
  User git
  IdentityFile ~/.ssh/id_rsa_personal
  IdentitiesOnly yes

# Private Git
Host gitlab.alex-gilbert.net
    HostName gitlab.alex-gilbert.net
    Port 2222
    AddKeysToAgent yes
    User git
    IdentityFile ~/.ssh/id_rsa_personal

# Private Git Direct
Host nas
    HostName nas
    Port 2222
    AddKeysToAgent yes
    User git
    IdentityFile ~/.ssh/id_rsa_personal
